{
    "name": "waa-language-support",
    "displayName": "WAA Language Support",
    "description": "This extension makes .waa files not just pure text... ;)",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "waa",
            "aliases": ["WAA", "waa"],
            "extensions": [".waa"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "waa",
            "scopeName": "source.waa",
            "path": "./syntaxes/waa.tmLanguage.json"
        }]
    }
}